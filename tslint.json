{
  "extends": [
    "tslint-eslint-rules",
    "tslint-config-standard"
  ],
  "rules": {
    "no-duplicate-imports": false,
    "no-empty": false,
    "no-floating-promises": false,
    "return-undefined": true,
    "typedef": [
      true,
      "call-signature",
      "property-declaration",
      "parameter",
      "member-variable-declaration"
    ]
  }
}