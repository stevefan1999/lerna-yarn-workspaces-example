{
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@wordpress/jest-console": "^2.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lerna": "^3.0.6",
    "lint-staged": "^7.2.2",
    "module-alias": "^2.1.0",
    "prettier": "^1.14.2",
    "prettier-tslint": "^0.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint packages/**/*.js --fix",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier-tslint fix packages/**/*.ts",
      "git add"
    ]
  },
  "name": "lerna-yarn-workspaces-example",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn clean && lerna run --parallel --stream build",
    "clean": "rimraf packages/**/dist",
    "lint": "eslint packages/**/*.js && tslint packages/**/*.ts",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "test": "jest",
    "watch": "lerna exec --parallel -- yarn watch"
  },
  "workspaces": [
    "packages/*"
  ]
}